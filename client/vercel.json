{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",  // Adjust if the client directory is elsewhere
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"  // Set to where your build output folder is
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"  // Adjust to correctly route requests to the built static files
    }
  ]
}
